{
  "manifest_version": 2,
  "name": "Random unusual new tab",
  "description": "Redirects to a random unusual wikipedia page when you open a new tab",
  "homepage_url": "https://github.com/mehrpouya/openbubble",
  "version": "1.0",
  "icons": {
    "48": "icons/openbubble.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["openbubble.js"]
  }
}
